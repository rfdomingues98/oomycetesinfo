<div class="modal fade" id="showAlignmentResults" tabindex="-1" role="dialog"
	aria-labelledby="showAlignmentResultsTitle" aria-hidden="true">
	<div class="modal-dialog modal-lg modal-dialog-centered" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="showAlignmentResultsTitle">Alignment Results</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				<div class="table-responsive">
					<table class="table table-striped table-bordered">
						<thead class="thead-dark">
							<th scope="col">BLAST</th>
							<th scope="col">UNITE</th>
							<th scope="col">Bold Systems</th>
							<th scope="col">PhytophthoraDB</th>
						</thead>
						<tbody id="region_alignment">
							<% if(data.blast != undefined && data.unite != undefined && data.bold != undefined && data.phyto != undefined) { %>
							<tr>
								<td><%= data.docs[i].blast %></td>
								<td><%= data.docs[i].unite %></td>
								<td><%= data.docs[i].bold %></td>
								<td><%= data.docs[i].phyto %>
							</tr>
							<% } %>
						</tbody>
					</table>
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
			</div>
		</div>
	</div>
</div>